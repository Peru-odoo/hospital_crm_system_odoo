<odoo>
    <data>
        <record id="view_report_wizard_form" model="ir.ui.view">
            <field name="name">hospital.report.wizard.form</field>
            <field name="model">hospital.report.wizard</field>
            <field name="arch" type="xml">
                <form>
                    <group>
                        <field name="start_date"/>
                        <field name="end_date"/>
                    </group>
                    <footer>
                        <button string="Generate Report" type="object" class="oe_highlight" name="print_disease_report"/>
                    </footer>
                </form>
            </field>
        </record>
    </data>
</odoo>